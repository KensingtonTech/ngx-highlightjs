{
  "name": "@kensingtontech/ngx-highlightjs",
  "version": "0.0.0",
  "description": "Instant code highlighting, auto-detect language.",
  "homepage": "https://github.com/KensingtonTech/ngx-highlightjs",
  "author": {
    "name": "Tim Underhay",
    "url": "http://github.com/KensingtonTech"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/KensingtonTech/ngx-highlightjs.git"
  },
  "keywords": [
    "angular",
    "highlight",
    "highlightjs",
    "prism",
    "syntax",
    "code"
  ],
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-highlightjs-demo",
    "build-lib": "ng build ngx-highlightjs --prod",
    "test-lib": "ng test ngx-highlightjs",
    "test-lib-headless": "ng test ngx-highlightjs --watch=false --no-progress --browsers=ChromeHeadless",
    "link-lib": "npm link dist/ngx-highlightjs && npm link ngx-highlightjs",
    "publish-lib": "npm publish dist/ngx-highlightjs",
    "e2e": "ng e2e",
    "buildtest": "npm run build-lib && cd dist/ngx-highlightjs && npm pack . && cp *.tgz ~/src/amos-client"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/core": "~11.0.2",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "rxjs": "~6.5.4",
    "tslib": "^2.2.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular-devkit/core": "^11.0.2",
    "@angular-devkit/schematics": "^11.0.2",
    "@angular/cli": "^11.0.2",
    "@angular/compiler-cli": "^11.0.2",
    "@angular/language-service": "^11.0.2",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "highlight.js": "^11.0.0-beta1",
    "highlightjs-line-numbers.js": "^2.8.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "ng-packagr": "^11.0.3",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
